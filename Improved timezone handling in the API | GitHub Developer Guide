.tooltipped-no-delay:active::after, .tooltipped-no-delay:active::before, .tooltipped-no-delay:focus::after, .tooltipped-no-delay:focus::before, .tooltipped-no-delay:hover::after, .tooltipped-no-delay:hover::before { opacity: 1; animation: auto ease 0s 1 normal none running none; }

