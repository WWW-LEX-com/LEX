  .mw-parser-output .sticky-header.static-row-numbers.wikitable > tbody > tr:not(.static-row-header)::before, .mw-parser-output .sticky-header-multi.static-row-numbers.wikitable > tbody > tr:not(.static-row-header)::before { border-bottom-width: 0px !important; border-right-width: 0px !important; }
  body.skin-timeless .mw-parser-output .content-table-scrollbar, body.skin-timeless .mw-parser-output .overflowed, body.skin-timeless .mw-parser-output .overflowed .content-table { overflow: visible; }
