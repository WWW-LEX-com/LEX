.cnotice { margin-bottom: 0px !important; }

.mw-parser-output .infobox-header, .mw-parser-output .infobox-subheader, .mw-parser-output .infobox-above, .mw-parser-output .infobox-title, .mw-parser-output .infobox-image, .mw-parser-output .infobox-full-data, .mw-parser-output .infobox-below { text-align: center; }

.mw-parser-output .nowrap, .nowraplinks a { white-space: nowrap; }

.mw-parser-output .infobox .nowrap { white-space: normal !important; }

.wrap, .wraplinks a { white-space: normal; }

span.texhtml { font-family: serif; white-space: nowrap; }

.wikitable.plainrowheaders th[scope="row"], .wikitable.plainrowheaders th[scope="rowgroup"] { font-weight: normal; text-align: left; }

